# Server Configuration
PORT=3001

# Printer Configuration
# PRINTER_TYPE: USB or NETWORK
PRINTER_TYPE=USB

# For USB printers on Windows, use printer name from Windows (e.g., "POS-80" or leave empty for auto-detect)
# For NETWORK printers, use IP address
PRINTER_INTERFACE=
DEFAULT_PRINTER_IP=192.168.1.100

# Printer Model: EPSON or STAR
PRINTER_MODEL=EPSON

# Cash Drawer Configuration
ENABLE_CASH_DRAWER=true
AUTO_OPEN_DRAWER=true